{
    "name": "emaia/laravel-mediaman",
    "license": "MIT",
    "description": "MediaMan - The most elegant & powerful media management package for Laravel!",
    "keywords": [
        "laravel package",
        "laravel media",
        "laravel mediaman",
        "laravel media collection",
        "laravel media management",
        "laravel file management",
        "laravel media manager",
        "laravel file manager"
    ],
    "homepage": "https://github.com/FarhanShares/laravel-mediaman",
    "authors": [
        {
            "name": "Farhan Israq",
            "email": "farhan.israq.bd@gmail.com",
            "homepage": "https://farhanshares.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^12.0",
        "illuminate/database": "^12.0",
        "illuminate/validation": "^12.0",
        "intervention/image": "^3.0"
    },
    "require-dev": {
        "laravel/framework": "^12.0",
        "orchestra/testbench": "^10.0",
        "mockery/mockery": "^1.2.3",
        "laravel/legacy-factories": "^1.3.9"
    },
    "autoload": {
        "psr-4": {
            "FarhanShares\\MediaMan\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FarhanShares\\MediaMan\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "FarhanShares\\MediaMan\\MediaManServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
