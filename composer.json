{
    "name": "emaia/laravel-mediaman",
    "license": "MIT",
    "description": "MediaMan - The most elegant & powerful media management package for Laravel!",
    "keywords": [
        "laravel package",
        "laravel media",
        "laravel mediaman",
        "laravel media collection",
        "laravel media management",
        "laravel file management",
        "laravel media manager",
        "laravel file manager"
    ],
    "homepage": "https://github.com/emaia/laravel-mediaman",
    "authors": [
        {
            "name": "Emaia",
            "email": "eu@emaia.dev",
            "homepage": "https://emaia.dev",
            "role": "Developer"
        },
        {
            "name": "Farhan Israq",
            "email": "farhan.israq.bd@gmail.com",
            "homepage": "https://farhanshares.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^12.0",
        "illuminate/database": "^12.0",
        "illuminate/validation": "^12.0",
        "intervention/image": "^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.0",
        "orchestra/workbench": "^10.0",
        "laravel/legacy-factories": "^1.3.9",
        "pestphp/pest": "^3.8",
        "mockery/mockery": "^1.6",
        "pestphp/pest-plugin-drift": "^3.0",
        "larastan/larastan": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Emaia\\MediaMan\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Emaia\\MediaMan\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Emaia\\MediaMan\\MediaManServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "test" : "./vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    }
}
